{
  "cli": {
    "version": ">= 16.17.3",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "NODE_OPTIONS": "--max-old-space-size=4096",
        "EXPO_DEBUG": "1"
      },
      "channel": "preview"
    },
    "preview-debug": {
      "distribution": "internal", 
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "env": {
        "NODE_OPTIONS": "--max-old-space-size=4096",
        "EXPO_DEBUG": "1"
      },
      "developmentClient": false
    }
  },
  "submit": {
    "production": {}
  }
}
