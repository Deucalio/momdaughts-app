{
  "name": "react-native-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@rneui/base",
          "@rneui/themed",
          "autoprefixer",
          "geist",
          "input-otp",
          "jose",
          "lucide-react",
          "nanoid",
          "prettier-plugin-tailwindcss",
          "tailwindcss-animate"
        ]
      }
    }
  },
  "scripts": {
    "build:dev": "npm install --force --legacy-peer-deps && eas build --profile development",
    "build:dev:android": "npm install --force --legacy-peer-deps && eas build --profile development --platform android",
    "build:dev:ios": "npm install --force --legacy-peer-deps && eas build --profile development --platform ios",
    "build:preview": "npm install --force --legacy-peer-deps && eas build --profile preview",
    "build:prod": "npm install --force --legacy-peer-deps && eas build --profile production",
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@hookform/resolvers": "^3.9.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.4.4",
    "@react-navigation/drawer": "latest",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.16",
    "@react-navigation/stack": "^7.4.4",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "~53.0.20",
    "expo-auth-session": "~6.2.1",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linear-gradient": "latest",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.4",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "input-otp": "1.4.1",
    "jose": "^6.0.12",
    "nanoid": "^3.3.6",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.1",
    "react-native": "0.79.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-swiper": "^1.6.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "^13.13.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "^19.1.1",
    "@types/react": "~19.0.10",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-import": "^2.32.0",
    "expo": "^53.0.20",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3"
  },
  "private": true
}
